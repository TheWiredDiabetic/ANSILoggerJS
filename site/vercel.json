{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
        "src":"/assets/:folder/:asset",
        "dest":"/assets/:folder/:asset"
      }
    ]
  }
  